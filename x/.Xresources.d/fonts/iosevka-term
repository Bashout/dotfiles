! Iosevka Term Font Definitions
! -----------------------------

! Default Font
#define fontName Iosevka\ Term
#define fontSize 14

#define fontNameB Iosevka\ Term
#define fontSizeB 14

#define fontNameI Iosevka\ Term
#define fontSizeI 14

#define fontNameBI Iosevka\ Term
#define fontSizeBI 14

! Fallback Font
#define fontFbName Monospace
#define fontFbSize 14

#define fontFbNameB Monospace
#define fontFbSizeB 14

#define fontFbNameI Monospace
#define fontFbSizeI 14

#define fontFbNameBI Monospace
#define fontFbSizeBI 14

! Either Font
#define fontUseB    true
#define fontUseI    true
#define fontSpacing 1


